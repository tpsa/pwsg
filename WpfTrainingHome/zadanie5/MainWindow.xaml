<Window x:Class="zadanie5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <ToolBarTray DockPanel.Dock='Top'>
            <ToolBar>
                <Button Click="Button_Click_1">
                    <Button.LayoutTransform>
                        <RotateTransform Angle="{Binding DateTime.Now.Seconds, Path=Value, Mode=OneWay}" />
                    </Button.LayoutTransform>
                    Prev</Button>
                <Button Click="Button_Click_2">Next</Button>
            </ToolBar>
            <ToolBar HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">
                <TextBox Name="AddressTextBox" Text="http://www.google.com" HorizontalAlignment="Stretch" />
                <Button Width='23' Click="Button_Click">Go</Button>
            </ToolBar>
        </ToolBarTray>
        <WebBrowser Name="ContentWebBrowser"></WebBrowser>
    </DockPanel>
</Window>
